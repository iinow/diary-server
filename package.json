{
  "name": "diary-server",
  "version": "1.0.0",
  "description": "graphQL 예제",
  "main": "index.js",
  "scripts": {
    "build:local": "cross-env TS_NODE_PROJECT=\"build/tsconfig-webpack.json\" npx webpack --config build/webpack.local.config.ts",
    "build:dev": "npx webpack --config build/webpack.dev.config.js",
    "build:real": "npx webpack --config build/webpack.real.config.js",
    "migrate:local:create": "npx db-migrate create seed --config build/_db/config/local.json -e local -m build/_db/migrations",
    "migrate:local-up": "npx db-migrate up seed --config build/_db/config/local.json -e local -m build/_db/migrations",
    "migrate:local-down": "npx db-migrate down seed --config build/_db/config/local.json -e local -m build/_db/migrations"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.1.5",
    "@types/moment": "^2.13.0",
    "@types/redis": "^2.8.27",
    "apollo-server": "^2.17.0",
    "apollo-server-express": "^2.17.0",
    "axios": "^0.20.0",
    "chance": "^1.1.7",
    "class-validator": "^0.12.2",
    "cross-env": "^7.0.2",
    "db-migrate": "^0.11.11",
    "db-migrate-mysql": "^2.1.1",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "graphql": "^15.3.0",
    "graphql-subscriptions": "^1.1.0",
    "http": "0.0.1-security",
    "http2": "^3.3.7",
    "lodash-es": "^4.17.15",
    "moment": "^2.29.0",
    "mysql2": "^2.2.5",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "sqlite3": "^5.0.0",
    "type-graphql": "^1.0.0",
    "typeorm": "^0.2.26"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chance": "^1.1.0",
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.1",
    "@types/webpack": "^4.41.13",
    "@types/webpack-merge": "^4.1.5",
    "nodemon": "^2.0.3",
    "nodemon-webpack-plugin": "^4.0.3",
    "prettier": "^2.0.5",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
