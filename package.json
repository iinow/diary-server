{
  "name": "diary-server",
  "version": "1.0.0",
  "description": "Diary Api Server",
  "main": "index.js",
  "scripts": {
    "local": "cross-env TS_NODE_PROJECT=\"build/tsconfig-webpack.json\" npx webpack --config build/webpack.local.config.ts -w",
    "build:local": "cross-env TS_NODE_PROJECT=\"build/tsconfig-webpack.json\" npx webpack --config build/webpack.local.config.ts",
    "build:dev": "cross-env TS_NODE_PROJECT=\"build/tsconfig-webpack.json\" npx webpack --config build/webpack.dev.config.ts",
    "build:real": "npx webpack --config build/webpack.real.config.js",
    "migrate:local:create": "npx db-migrate create seed --config build/_db/config/local.json -e local -m build/_db/migrations",
    "migrate:local-up": "npx db-migrate up seed --config build/_db/config/local.json -e local -m build/_db/migrations",
    "migrate:local-down": "npx db-migrate down seed --config build/_db/config/local.json -e local -m build/_db/migrations"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^2.17.0",
    "apollo-server-express": "^2.17.0",
    "axios": "^0.20.0",
    "chance": "^1.1.7",
    "class-validator": "^0.12.2",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "db-migrate": "^0.11.11",
    "db-migrate-mysql": "^2.1.1",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "express-session": "^1.17.1",
    "graphql": "^15.3.0",
    "graphql-redis-subscriptions": "^2.3.1",
    "graphql-subscriptions": "^1.1.0",
    "http": "0.0.1-security",
    "http2": "^3.3.7",
    "ioredis": "^4.19.2",
    "jsonwebtoken": "^8.5.1",
    "lodash-es": "^4.17.15",
    "moment": "^2.29.0",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-kakao": "^1.0.0",
    "passport-oauth": "^1.0.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "type-graphql": "^1.0.0",
    "typeorm": "^0.2.26",
    "uuid": "^8.3.2"
  },
